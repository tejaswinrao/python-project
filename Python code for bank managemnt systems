import sqlite3 
conn= sqlite3.connect('D:\project.db')
print("database created and opened successfully")
conn.execute("CREATE TABLE CD(ATMPIN INT,,Username TEXT NOT NULL)")
print("table created successfully")
conn.close
import sqlite3
conn=sqlite3.connect('D:\project.db')
print("opened database successfully")
conn.execute("INSERT INTO CD(ATMPIN,Username)\ Values(1234, ASD);")
