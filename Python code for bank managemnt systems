import sqlite3 
conn= sqlite3.connect('D:\project.db')
print("database created and opened successfully")
conn.execute("CREATE TABLE PD(AGE INT,  NAME TEXT NOT NULL)")
print("table created successfully")
conn.close
import sqlite3
conn=sqlite3.connect('D:\project.db')
print("opened database successfully")
conn.execute("INSERT INTO PD(AGE,NAME)\ Values(28, 'BENNY');")
conn.execute("INSERT INTO PD(AGE,NAME)\ Values(38, 'chaiths');")
conn.execute("INSERT INTO PD(AGE,NAME)\ Values(48, 'joey');")
conn.execute("INSERT INTO PD(AGE,NAME)\ Values(58, 'linkin park');")
conn.execute("INSERT INTO PD(AGE,NAME)\ Values(68, 'hardwell');")
conn.execute("INSERT INTO PD(AGE,NAME)\ Values(78, 'Hummels');")
conn.execute("INSERT INTO PD(AGE,NAME)\ Values(88, 'shakira');")
conn.execute("INSERT INTO PD(AGE,NAME)\ Values(98, 'PAUL WALKER');")
conn.execute("INSERT INTO PD(AGE,NAME)\ Values(77, 'vin diesel');")
con.commit()
print("records created successfully");
conn.close()



